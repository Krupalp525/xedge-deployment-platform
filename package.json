  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/xedge.git"
  },
  "keywords": [
    "edge",
    "deployment",
    "management",
    "workflow",
    "react",
    "typescript",
    "express",
    "reactflow"
  ],
  "bugs": {
    "url": "https://github.com/yourusername/xedge/issues"
  },
  "homepage": "https://github.com/yourusername/xedge#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@tailwindcss/postcss": "^4.0.13",
    "@types/lodash": "^4.17.16",
    "lodash": "^4.17.21"
  }
}
